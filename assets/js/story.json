    "story": {
        "sequences": [
            {
                "sequenceId": 0,
                "sequenceName": "Breakfast with Mama",
                "locationId": 0,
                "scenes": [
                    {
                        "sceneId": 0,
                        "text": "November 4th 2019",
                        "sceneImage": "calendar_white.png",
                        "options": [
                            {
                                "optionText": "Okay",
                                "nextScene": 1,
                                "dependantVariables": []
                            }
                        ]
                    }, 
                    {
                        "sceneId": 1,
                        "text": "You wake with a fright. The memory of that awful scene still haunts you every night",
                        "sceneImage": "eyeball_white.png",
                        "options": [
                            {
                                "optionText": "Okay",
                                "nextScene": 2,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 2,
                        "text": "You go downstairs to get some breakfast. You call for your mother.",
                        "sceneImage": "stairs_white.png",
                        "options": [
                            {
                                "optionText": "Mama",
                                "nextScene": 3,
                                "dependantVariables": []
                            }, 
                            {
                                "optionText": "Hannah",
                                "nextScene": 3,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 3,
                        "text": "You hear a flurry of footsteps, more than one set of feet, which surprises you given your father's recent suicide and that no one else should be in the house. You open the fridge and find a bottle of milk, it's one day past expiry",
                        "sceneImage": "milk_white.png",
                        "options": [
                            {
                                "optionText": "Chance it",
                                "nextScene": 4,
                                "dependantVariables": []
                            },
                            {
                                "optionText": "Get toast instead",
                                "nextScene": 4,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 4,
                        "text": "Your mother walks down the stairs, she looks surprised that you are up. 'Why are you up Jonas?'",
                        "sceneImage": "character_full_white.png",
                        "options": [
                            {
                                "optionText": "It's the first day of school.",
                                "nextScene": 5,
                                "dependantVariables": []
                            },
                            {
                                "optionText": "I'm going to hang out with Marthat and Bartosz",
                                "nextScene": 5,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 5,
                        "text": "'You know you don't have to go to school if you don't want to, you still need time to heal' As she walks by you detect the faint smell of aftershave on her neck, certainly not one that you recognise as belonging to your father.",
                        "sceneImage": "perfume_white.png",
                        "options": [
                            {
                                "optionText": "I need to get out of the house.",
                                "nextScene": 6,
                                "dependantVariables": []
                            },
                            {
                                "optionText": "Why is the Milk gone off?",
                                "nextScene": 7,
                                "dependantVariables": []
                            },
                            {
                                "optionText": "Was there someone upstairs with you?",
                                "nextScene": 8,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 6,
                        "text": "'Okay, look after yourself' Your mother kisses you on the forehead.",
                        "sceneImage": "hearts_white.png",
                        "options": [
                            {
                                "optionText": "Okay",
                                "nextScene": 10,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 7,
                        "text": "'I didn't get a chance to go to the shop yesterday' Your mother glares at you.",
                        "sceneImage": "character_full_white.png",
                        "options": [
                            {
                                "optionText": "Okay",
                                "nextScene": 10,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 8,
                        "text": "'Wha... What are you talking about?' Your mother spins and glares at you, shaking the coffee container she's holding",
                        "sceneImage": "suspicious_white.png",
                        "options": [
                            {
                                "optionText": "I heard footsteps upstairs",
                                "nextScene": 9,
                                "dependantVariables": []
                            },
                            {
                                "optionText": "Nevermind",
                                "nextScene": 10,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 9,
                        "text": "'You must be imagining things' Your mother says nervously. 'You still see your dead father walking around, forget it'",
                        "sceneImage": "character_white.png",
                        "options": [
                            {
                                "optionText": "Okay",
                                "nextScene": 10,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 10,
                        "text": "You finish your breakfast. Eager to get going, you rush back upstairs and grab your school things and make for the front door.",
                        "sceneImage": "stairs_white.png",
                        "options": [
                            {
                                "optionText": "Okay",
                                "nextScene": 11,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 11,
                        "text": "You hear your mother call out to you. 'Don't forget your yellow jacket, it's pouring outside!'",
                        "sceneImage": "jacket_white.png",
                        "options": [
                            {
                                "optionText": "Okay",
                                "nextScene": -1,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 12,
                        "text": "There is no one at home now",
                        "sceneImage": "kahnwald_house_white.png",
                        "options": [
                            {
                                "optionText": "Leave",
                                "nextScene": -1,
                                "dependantVariables": []
                            }
                        ]
                    }
                ]
            },
            {
                "sequenceId": 1,
                "sequenceName": "Ulrich Encounter",
                "locationId": 1,
                "scenes": [
                    {
                        "sceneId": 0,
                        "text": "As you approach the Nielsen house you see a middle aged man exit the house. You recognise him as Ulrich Nielsen.",
                        "sceneImage": "character_full_white.png",
                        "options": [
                            {
                                "optionText": "Okay",
                                "nextScene": 1,
                                "dependantVariables": []
                            }
                        ]
                    }, 
                    {
                        "sceneId": 1,
                        "text": "As you approach, Ulrich spies you out of the corner of his eye. He turns and examines you. 'Hello Jonas, shouldn't you be in school?'",
                        "sceneImage": "police_officer_head_white.png",
                        "options": [
                            {
                                "optionText": "I'm sick",
                                "nextScene": 2,
                                "dependantVariables": []
                            },
                            {
                                "optionText": "I needed some space",
                                "nextScene": 3,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 2,
                        "text": "'You don't look sick' remarks Ulrich. He stares at you intently for half a second before breaking into a smile. 'I'm only joking Jonas, I won't arrest you today!'",
                        "sceneImage": "suspicious_white.png",
                        "options": [
                            {
                                "optionText": "That's good to hear",
                                "nextScene": 4,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 3,
                        "text": "'I understand' remarks Ulrich. 'You've been through a lot these last few months'",
                        "sceneImage": "character_white.png",
                        "options": [
                            {
                                "optionText": "Okay",
                                "nextScene": 4,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 4,
                        "text": "Ulrich approaches you and as he does you detect a familiar smell. The aftershave is the same that you smelt on your mother at breakfast",
                        "sceneImage": "perfume_white.png",
                        "options": [
                            {
                                "optionText": "Were you at our house earlier?",
                                "nextScene": 5,
                                "dependantVariables": []
                            },
                            {
                                "optionText": "Where are you going?",
                                "nextScene": 9,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 5,
                        "text": "Ulrich's eyes suddenly narrow as he glares at you, studying. 'No' he says 'Why would you think that?'",
                        "sceneImage": "angry-eyes.png",
                        "options": [
                            {
                                "optionText": "The aftershave, I smelt it on my mother this morning and I heard two sets of footsteps when I called her.",
                                "nextScene": 6,
                                "dependantVariables": []
                            },
                            {
                                "optionText": "Nevermind",
                                "nextScene": 8,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 6,
                        "text": "Ulrich leans in close. 'I don't know what games you think you are playing but if you so much as speak a word of this to anyone in my family I'll lock you up in jail for a week.'",
                        "sceneImage": "shadow-follower.png",
                        "options": [
                            {
                                "optionText": "I'm going to tell Katharina",
                                "nextScene": 7,
                                "dependantVariables": []
                            },
                            {
                                "optionText": "Nevermind",
                                "nextScene": 8,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 7,
                        "text": "Ulrich shoves you against the bonnet of his car. 'Jonas you are under arrest!'",
                        "sceneImage": "arrest.png",
                        "options": [
                            {
                                "optionText": "Okay",
                                "nextScene": "Jail End",
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 8,
                        "text": "Ulrich eyes you suspiciously before breaking into a smile again. 'Nevermind, Jonas. Why don't you run along to school and talk with Martha and Magnus'",
                        "sceneImage": "police_officer_head_white.png",
                        "options": [
                            {
                                "optionText": "Okay",
                                "nextScene": 9,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 9,
                        "text": "'I have to go into work' says Ulrich, somberly. 'We still have no leads on where Erik is.'",
                        "sceneImage": "worried-eyes.png",
                        "options": [
                            {
                                "optionText": "Where do you think he's gone?",
                                "nextScene": 10,
                                "dependantVariables": []
                            },
                            {
                                "optionText": "Well I'm off",
                                "nextScene": 12,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 10,
                        "text": "'We don't know, we've searched the forest where he was last seen but have found nothing, as far as we can tell he's just disappeared off the face of the Earth' Ulrich's voice gains a tone of sadness. 'It's like my brother, Mads' case all over again'",
                        "sceneImage": "magnifying-glass.png",
                        "options": [
                            {
                                "optionText": "He disappeared 33 years ago didn't he?",
                                "nextScene": 11,
                                "dependantVariables": []
                            },
                            {
                                "optionText": "Well I'm off",
                                "nextScene": 12,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 11,
                        "text": "'Yes, the circumstances seem to match up nearly exactly. It's very strange' Ulrich ponders this for a moment. 'Anyway, I must go.'",
                        "sceneImage": "idea.png",
                        "options": [
                            {
                                "optionText": "Okay",
                                "nextScene": 12,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 12,
                        "text": "'See you around Jonas' Ulrich says as he hops in his car and departs.",
                        "sceneImage": "city-car.png",
                        "options": [
                            {
                                "optionText": "See you around",
                                "nextScene": -1,
                                "dependantVariables": []
                            }
                        ]
                    }, 
                    {
                        "sceneId": 13,
                        "text": "The door is locked. There is no one home",
                        "sceneImage": "nielsen_house_white.png",
                        "options": [
                            {
                                "optionText": "Okay",
                                "nextScene": -1,
                                "dependantVariables": []
                            }
                        ]
                    }
                ]
            },
            {
                "sequenceId": 2,
                "sequenceName": "Charlotte Doppler",
                "locationId": 2,
                "scenes": [
                    {
                        "sceneId": 0,
                        "text": "As you approach the Police Station a short haired, middle aged woman exits the front door. She seems troubled. You recognise her as Charlotte Doppler.",
                        "sceneImage": "character_full_white.png",
                        "options": [
                            {
                                "optionText": "Okay",
                                "nextScene": 1,
                                "dependantVariables": []
                            }
                        ]
                    }, 
                    {
                        "sceneId": 1,
                        "text": "Charlotte looks up from the paper she's holding as she spots you. 'Hello Jonas'. The smile she wears seems forced.",
                        "sceneImage": "cowled.png",
                        "options": [
                            {
                                "optionText": "Hi Charlotte",
                                "nextScene": 2,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 2,
                        "text": "'What are you doing here?' asks Charlotte.",
                        "sceneImage": "discussion.png",
                        "options": [
                            {
                                "optionText": "I wanted to ask how Erik's investigation is going",
                                "nextScene": 3,
                                "dependantVariables": []
                            },
                            {
                                "optionText": "Just passing through",
                                "nextScene": 7,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 3,
                        "text": "'Ohh, well you know I can't discuss an active investigation with a civilian.' Charlotte looks curiously at you. 'Why do you ask? Do you know something that could help us?'",
                        "sceneImage": "conversation.png",
                        "options": [
                            {
                                "optionText": "Yes, I'd like to confess. I killed Erik.",
                                "nextScene": "Jail End",
                                "dependantVariables": []
                            },
                            {
                                "optionText": "No I'd just like to know what happened",
                                "nextScene": 4,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 4,
                        "text": "Charlotte sighs. 'I'm afraid we don't know anymore than what's already common knowledge. Erik left home on the evening of November 1st to go into the forest and he hasn't been seen since.'",
                        "sceneImage": "worried-eyes.png",
                        "options": [
                            {
                                "optionText": "Okay",
                                "nextScene": 5,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 5,
                        "text": "'We were told by a few of his friends that he liked to frequent the cave in the middle of the forest but that cave is a maze and I'm afraid if he did go in there that it would be nearly impossible to find him.",
                        "sceneImage": "cave_white.png",
                        "options": [
                            {
                                "optionText": "Okay",
                                "nextScene": 6,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 6,
                        "text": "'His parents said he was wearing a yellow shirt, blue jeans and a necklace with a blue pendant on the end. So, if you see someone matching that description, please tell us immediately'",
                        "sceneImage": "feather-necklace.png",
                        "options": [
                            {
                                "optionText": "Okay",
                                "nextScene": 7,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 7,
                        "text": "'I have to go Jonas, I have more witnesses to question' Charlotte hurriedly walks off.",
                        "sceneImage": "walk.png",
                        "options": [
                            {
                                "optionText": "Okay",
                                "nextScene": -1,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 8,
                        "text": "The station is closed.",
                        "sceneImage": "police_station_white.png",
                        "options": [
                            {
                                "optionText": "Okay",
                                "nextScene": -1,
                                "dependantVariables": []
                            }
                        ]
                    }
                ]
            }, 
            {
                "sequenceId": 3,
                "sequenceName": "Martha Encounter",
                "locationId": 8,
                "scenes": [
                    {
                        "sceneId": 0,
                        "text": "You enter the forest, the terrain is mostly easy and forgiving. The forest appears empty, you hear only the odd chirps of birds.",
                        "sceneImage": "character_full_white.png",
                        "options": [
                            {
                                "optionText": "Okay",
                                "nextScene": 1,
                                "dependantVariables": []
                            }
                        ]
                    }, 
                    {
                        "sceneId": 1,
                        "text": "You continue strolling through the forest for a few minutes when you hear a branch break somewhere in the distance.",
                        "sceneImage": "police_officer_head_white.png",
                        "options": [
                            {
                                "optionText": "Investigate",
                                "nextScene": 2,
                                "dependantVariables": []
                            },
                            {
                                "optionText": "Ignore",
                                "nextScene": 3,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 2,
                        "text": "As you approach the noise you began to make out a familiar figure, Martha Nielsen",
                        "sceneImage": "suspicious_white.png",
                        "options": [
                            {
                                "optionText": "Okay",
                                "nextScene": 3,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 3,
                        "text": "Martha hears you as you approach and looks up from the book in her hand to catch your gaze.",
                        "sceneImage": "character_white.png",
                        "options": [
                            {
                                "optionText": "Okay",
                                "nextScene": 4,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 4,
                        "text": "'Jonas!' She jumps up from the stump she's sitting in and runs to you open armed. Before you know it she has you in a tight embrace.",
                        "sceneImage": "perfume_white.png",
                        "options": [
                            {
                                "optionText": "Embrace back",
                                "nextScene": 5,
                                "dependantVariables": []
                            },
                            {
                                "optionText": "Don't",
                                "nextScene": 5,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 5,
                        "text": "'I missed you so much all summer! It just wasn't the same without you around'",
                        "sceneImage": "angry-eyes.png",
                        "options": [
                            {
                                "optionText": "I missed you too",
                                "nextScene": 6,
                                "dependantVariables": []
                            },
                            {
                                "optionText": "I needed space from this place",
                                "nextScene": 6,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 6,
                        "text": "Martha stares at you quizzingly. 'Where were you? Bartosz said you were in France but I know that was bullshit.'",
                        "sceneImage": "shadow-follower.png",
                        "options": [
                            {
                                "optionText": "I was in Frace visiting relatives",
                                "nextScene": 7,
                                "dependantVariables": []
                            },
                            {
                                "optionText": "I was in a psychiatric hospital",
                                "nextScene": 9,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 7,
                        "text": "Martha frowns. 'You're not convincing me. Why can't you just tell me where you really were? I know it was hard after your dad died but I could've helped and been there for you but you just shut me out!'",
                        "sceneImage": "arrest.png",
                        "options": [
                            {
                                "optionText": "I really was in France",
                                "nextScene": 8,
                                "dependantVariables": []
                            },
                            {
                                "optionText": "I was in a psychiatric hospital",
                                "nextScene": 9,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 8,
                        "text": "'Fine, don't tell me, hide secrets. I thought we were closer than that.' A tear comes to Martha's eye as she storms off.",
                        "sceneImage": "police_officer_head_white.png",
                        "options": [
                            {
                                "optionText": "Okay",
                                "nextScene": 20,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 9,
                        "text": "'What?' Martha asks disbelieving. 'No, it makes sense, you were so distraught after your father's death. Jonas, I'm so sorry. That must have been so hard on you.'",
                        "sceneImage": "worried-eyes.png",
                        "options": [
                            {
                                "optionText": "It was",
                                "nextScene": 10,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 10,
                        "text": "'We can talk about it if you want.'",
                        "sceneImage": "magnifying-glass.png",
                        "options": [
                            {
                                "optionText": "I want to talk about it",
                                "nextScene": 11,
                                "dependantVariables": []
                            },
                            {
                                "optionText": "I don't want to talk about it",
                                "nextScene": 14,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 11,
                        "text": "'What was it like?'",
                        "sceneImage": "idea.png",
                        "options": [
                            {
                                "optionText": "It was hard, I was so angry at everything and everyone",
                                "nextScene": 12,
                                "dependantVariables": []
                            },
                            {
                                "optionText": "It was fine",
                                "nextScene": 14,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 12,
                        "text": "'That sounds horrible. I know how close you were to your father'",
                        "sceneImage": "city-car.png",
                        "options": [
                            {
                                "optionText": "I miss him every day",
                                "nextScene": 13,
                                "dependantVariables": []
                            },
                            {
                                "optionText": "I've forgotten him already",
                                "nextScene": 14,
                                "dependantVariables": []
                            }
                        ]
                    }, 
                    {
                        "sceneId": 13,
                        "text": "'I'm so sorry Jonas'",
                        "sceneImage": "nielsen_house_white.png",
                        "options": [
                            {
                                "optionText": "It's okay, I'm glad you're here",
                                "nextScene": 15,
                                "dependantVariables": []
                            }
                        ]
                    }, 
                    {
                        "sceneId": 14,
                        "text": "'Ohh okay' Martha looks at you oddly.",
                        "sceneImage": "nielsen_house_white.png",
                        "options": [
                            {
                                "optionText": "Say nothing",
                                "nextScene": 15,
                                "dependantVariables": []
                            }
                        ]
                    },
                    {
                        "sceneId": 15,
                        "text": "'Can I ask you something Jonas?'",
                        "sceneImage": "nielsen_house_white.png",
                        "options": [
